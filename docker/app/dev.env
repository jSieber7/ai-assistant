# Build mode for the FastAPI app
BUILD_MODE=development

# Port for the FastAPI app
APP_PORT=8000

# Base domain for Traefik routing
BASE_DOMAIN=localhost

# Compose project name
COMPOSE_PROJECT_NAME=my-stack

# Custom Reranker Configuration
CUSTOM_RERANKER_ENABLED=true
CUSTOM_RERANKER_MODEL=all-MiniLM-L6-v2