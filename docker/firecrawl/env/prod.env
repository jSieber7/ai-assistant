############
# Firecrawl Environment Variables
# Copy this file to .env and update the values as needed
############

# Redis Configuration
REDIS_URL=${REDIS_URL:-redis://redis:6379}
REDIS_RATE_LIMIT_URL=${REDIS_URL:-redis://redis:6379}

# Playwright Service
PLAYWRIGHT_MICROSERVICE_URL=${PLAYWRIGHT_MICROSERVICE_URL:-http://playwright-service:3000/scrape}

# Database Configuration
NUQ_DATABASE_URL=postgres://postgres:postgres@nuq-postgres:5432/postgres
USE_DB_AUTHENTICATION=true

# AI/ML Configuration
OPENAI_API_KEY=your-production-openai-api-key
OPENAI_BASE_URL=https://api.openai.com/v1
MODEL_NAME=gpt-4
MODEL_EMBEDDING_NAME=text-embedding-ada-002
OLLAMA_BASE_URL=http://ollama:11434

# Notification Configuration
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/PRODUCTION/SLACK/WEBHOOK

# Authentication & API Keys
BULL_AUTH_KEY=your-production-bull-auth-key
TEST_API_KEY=your-production-test-api-key

# Analytics Configuration
POSTHOG_API_KEY=your-production-posthog-api-key
POSTHOG_HOST=https://app.posthog.com

# Supabase Configuration
SUPABASE_ANON_TOKEN=your-production-supabase-anon-token
SUPABASE_URL=https://your-production-project.supabase.co
SUPABASE_SERVICE_TOKEN=your-production-supabase-service-token

# Webhook Configuration
SELF_HOSTED_WEBHOOK_URL=https://yourdomain.com/api/webhook

# Search API Configuration
SERPER_API_KEY=your-production-serper-api-key
SEARCHAPI_API_KEY=your-production-searchapi-api-key

# Logging Configuration
LOGGING_LEVEL=warn

# Proxy Configuration
PROXY_SERVER=
PROXY_USERNAME=
PROXY_PASSWORD=

# SearXNG Configuration
SEARXNG_ENDPOINT=http://searxng:8080
SEARXNG_ENGINES=google,bing,duckduckgo
SEARXNG_CATEGORIES=general