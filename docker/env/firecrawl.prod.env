# ===========================================
# FIRECRAWL PRODUCTION OVERRIDES
# ===========================================

# Logging
LOGGING_LEVEL=info

# Ports (keep internal, use Traefik for routing)
PORT=3002
EXTRACT_WORKER_PORT=3004
WORKER_PORT=3005

# Use internal service names
PLAYWRIGHT_MICROSERVICE_URL=http://my-stack-firecrawl-playwright:3000/scrape
SEARXNG_ENDPOINT=http://my-stack-searxng:8080

# Database (use local postgres)
NUQ_DATABASE_URL=postgres://postgres:postgres@my-stack-firecrawl-postgres:5432/postgres

# Feature Flags (enable authentication in production)
USE_DB_AUTHENTICATION=true

# Add your production API keys here
# OPENAI_API_KEY=your_prod_openai_key
# SUPABASE_ANON_TOKEN=your_supabase_anon_token
# SUPABASE_URL=your_supabase_url
# SUPABASE_SERVICE_TOKEN=your_supabase_service_token