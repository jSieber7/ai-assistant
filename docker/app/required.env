# =============================================================================
# AI Assistant - Missing Environment Variables
# =============================================================================
# This file contains environment variables used by the application that are
# not documented in the other example .env files. Copy any you need to your
# active .env file and provide appropriate values.

# =============================================================================
# Main Application Settings (from app/core/config.py)
# =============================================================================

# Server Configuration
HOST=127.0.0.1
PORT=8000
ENVIRONMENT=development
DEBUG=true
RELOAD=true

# Reverse Proxy Settings
BASE_URL=http://localhost
BEHIND_PROXY=true

# Tool and Agent System
TOOL_SYSTEM_ENABLED=true
MAX_CONCURRENT_TOOLS=5
AGENT_SYSTEM_ENABLED=true
DEFAULT_AGENT=
MAX_AGENT_ITERATIONS=3
AGENT_TIMEOUT=60

# Deep Agents System
DEEP_AGENTS_ENABLED=false

# Multi-Writer System
MULTI_WRITER_ENABLED=false

# Visual LMM System
VISUAL_SYSTEM_ENABLED=true
VISUAL_DEFAULT_MODEL=openai_vision:gpt-4-vision-preview
VISUAL_MAX_CONCURRENT_ANALYSES=3
VISUAL_SCREENSHOT_QUALITY=85
VISUAL_BROWSER_CONTROL_ENABLED=true

# Custom Reranker System
CUSTOM_RERANKER_ENABLED=true
CUSTOM_RERANKER_MODEL=all-MiniLM-L6-v2
CUSTOM_RERANKER_TIMEOUT=30
CUSTOM_RERANKER_CACHE_TTL=3600
CUSTOM_RERANKER_MAX_RETRIES=3

# Legacy Jina Reranker (for backward compatibility)
JINA_RERANKER_ENABLED=false
JINA_RERANKER_URL=http://jina-reranker:8080
JINA_RERANKER_MODEL=jina-reranker-v2-base-multilingual
JINA_RERANKER_TIMEOUT=30
JINA_RERANKER_CACHE_TTL=3600
JINA_RERANKER_MAX_RETRIES=3
JINA_RERANKER_API_KEY=

# LLM Provider Selection
PREFERRED_PROVIDER=openai_compatible
ENABLE_FALLBACK=true

# Backward Compatibility LLM Settings
OPENROUTER_API_KEY=
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
DEFAULT_MODEL=anthropic/claude-3.5-sonnet

# Unused/Deprecated Settings (kept for compatibility)
ROUTER_MODEL=deepseek/deepseek-chat
LOGIC_MODEL=anthropic/claude-3.5-sonnet
HUMAN_INTERFACE_MODEL=anthropic/claude-3.5-sonnet
POSTGRES_URL=
OPENAI_API_KEY=
SECRET_KEY=
SEARXNG_URL=http://searxng:8080

# =============================================================================
# OpenAI-Compatible Provider Settings (env_prefix: OPENAI_COMPATIBLE_)
# =============================================================================

OPENAI_COMPATIBLE_ENABLED=true
OPENAI_COMPATIBLE_API_KEY=
OPENAI_COMPATIBLE_BASE_URL=https://openrouter.ai/api/v1
OPENAI_COMPATIBLE_DEFAULT_MODEL=anthropic/claude-3.5-sonnet
OPENAI_COMPATIBLE_PROVIDER_NAME=
# For complex headers, use JSON format: OPENAI_COMPATIBLE_CUSTOM_HEADERS={"X-Custom-Header": "value"}
OPENAI_COMPATIBLE_CUSTOM_HEADERS={}
OPENAI_COMPATIBLE_TIMEOUT=30
OPENAI_COMPATIBLE_MAX_RETRIES=3

# =============================================================================
# Ollama Settings (env_prefix: OLLAMA_SETTINGS_)
# =============================================================================

OLLAMA_SETTINGS_ENABLED=true
OLLAMA_SETTINGS_BASE_URL=http://localhost:11434
OLLAMA_SETTINGS_DEFAULT_MODEL=llama2
OLLAMA_SETTINGS_TIMEOUT=30
OLLAMA_SETTINGS_MAX_RETRIES=3
OLLAMA_SETTINGS_TEMPERATURE=0.7
OLLAMA_SETTINGS_MAX_TOKENS=
OLLAMA_SETTINGS_STREAMING=true
OLLAMA_SETTINGS_HEALTH_CHECK_INTERVAL=60
OLLAMA_SETTINGS_AUTO_HEALTH_CHECK=true

# =============================================================================
# Firecrawl Settings (env_prefix: FIRECRAWL_)
# =============================================================================

FIRECRAWL_ENABLED=false
FIRECRAWL_DEPLOYMENT_MODE=docker
FIRECRAWL_DOCKER_URL=http://firecrawl:3002
FIRECRAWL_BULL_AUTH_KEY=
FIRECRAWL_SCRAPING_ENABLED=true
FIRECRAWL_MAX_CONCURRENT_SCRAPES=5
FIRECRAWL_SCRAPE_TIMEOUT=60
FIRECRAWL_CONTENT_CLEANING=true
FIRECRAWL_EXTRACT_IMAGES=false
FIRECRAWL_EXTRACT_LINKS=true
# For list formats, use JSON format: FIRECRAWL_FORMATS=["markdown", "raw"]
FIRECRAWL_FORMATS=["markdown", "raw"]
FIRECRAWL_WAIT_FOR=2000
FIRECRAWL_SCREENSHOT=false
# For list formats, use JSON format: FIRECRAWL_INCLUDE_TAGS=["article", "main"]
FIRECRAWL_INCLUDE_TAGS=["article", "main", "content"]
# For list formats, use JSON format: FIRECRAWL_EXCLUDE_TAGS=["nav", "footer"]
FIRECRAWL_EXCLUDE_TAGS=["nav", "footer", "aside", "script", "style"]
FIRECRAWL_HEALTH_CHECK_TIMEOUT=10

# =============================================================================
# Playwright Settings (env_prefix: PLAYWRIGHT_)
# =============================================================================

PLAYWRIGHT_ENABLED=false
PLAYWRIGHT_HEADLESS=true
PLAYWRIGHT_BROWSER_TYPE=chromium
PLAYWRIGHT_TIMEOUT=30
PLAYWRIGHT_VIEWPORT_WIDTH=1280
PLAYWRIGHT_VIEWPORT_HEIGHT=720
PLAYWRIGHT_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36

# =============================================================================
# Milvus Settings (env_prefix: MILVUS_)
# =============================================================================

MILVUS_ENABLED=true
MILVUS_HOST=milvus
MILVUS_PORT=19530
MILVUS_USER=
MILVUS_PASSWORD=
MILVUS_DATABASE=default
MILVUS_COLLECTION_PREFIX=deep_search_
MILVUS_EMBEDDING_DIMENSION=1536
MILVUS_INDEX_TYPE=HNSW
MILVUS_METRIC_TYPE=COSINE
MILVUS_CONNECTION_POOL_SIZE=10
MILVUS_CONNECTION_TIMEOUT=10
MILVUS_MAX_RETRIES=3
MILVUS_AUTO_CLEANUP=true
MILVUS_CLEANUP_DELAY=3600

# =============================================================================
# Cache Settings (env_prefix: CACHING_)
# =============================================================================

CACHING_ENABLED=true
CACHING_DEFAULT_TTL=300
CACHING_MAX_CACHE_SIZE=1000
# For list formats, use JSON format: CACHING_CACHE_LAYERS=["memory", "redis"]
CACHING_CACHE_LAYERS=["memory", "redis"]
CACHING_WRITE_THROUGH=true
CACHING_READ_THROUGH=true
CACHING_MEMORY_CACHE_MAX_SIZE=500
CACHING_MEMORY_CACHE_CLEANUP_INTERVAL=60
CACHING_REDIS_HOST=localhost
CACHING_REDIS_PORT=6379
CACHING_REDIS_DB=0
CACHING_REDIS_PASSWORD=
CACHING_REDIS_MAX_CONNECTIONS=10
CACHING_REDIS_CONNECTION_TIMEOUT=5
CACHING_REDIS_RETRY_ATTEMPTS=3
CACHING_COMPRESSION_ENABLED=true
CACHING_COMPRESSION_ALGORITHM=gzip
CACHING_COMPRESSION_LEVEL=6
CACHING_COMPRESSION_THRESHOLD=100
CACHING_BATCHING_ENABLED=true
CACHING_MAX_BATCH_SIZE=10
CACHING_MAX_BATCH_WAIT_TIME=0.1
CACHING_MAX_BATCH_QUEUE_SIZE=1000
CACHING_CONNECTION_POOLING_ENABLED=true
CACHING_MAX_HTTP_CONNECTIONS=10
CACHING_MAX_DATABASE_CONNECTIONS=5
CACHING_CONNECTION_TIMEOUT=5.0
CACHING_ACQUIRE_TIMEOUT=10.0
CACHING_MONITORING_ENABLED=true
CACHING_STATS_COLLECTION_INTERVAL=60